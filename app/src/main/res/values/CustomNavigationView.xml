<?xml version="1.0" encoding="utf-8"?>
<resources>
    <bool name="is_right_to_left">false</bool>
    <declare-styleable name="CustomNavigationView">
        <attr name="drawerShape" format="enum">
            <enum name="arcConvex" value="0" />
            <enum name="arcConcave" value="1" />
            <enum name="roundedRect" value="2" />
            <enum name="waves" value="3" />
            <enum name="bottom_round" value="4" />
            <enum name="full_round" value="5" />
            <enum name="waves_indefinite" value="6" />
            <enum name="end_corners_rounded" value="7" />
        </attr>

        <attr name="cornerRadius" format="dimension"/>
    </declare-styleable>

</resources>